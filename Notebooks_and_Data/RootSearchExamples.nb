   SuperscriptBox["10", "5"], 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"Abs", "[", "x", "]"}], "+", "$MinMachineNumber"}], ")"}], 
   "$MachineEpsilon"}]}]], "Input",
 Evaluatable->False,ExpressionUUID->"ad80a5ce-99d5-d246-a073-9799d64091e4"],


   RowBox[{"{", 
    RowBox[{"x", ",", "\[Pi]", ",", "4"}], "}"}]}], "]"}]], "Input",Expression\
UUID->"20af3014-026d-b540-b8db-6fbe4ce6a19a"],

Cell[BoxData[
 InterpretationBox[
  RowBox[{
   RowBox[{
    FractionBox["1", "2"], " ", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"x", "-", "\[Pi]"}], ")"}], "2"]}], "+", 
   RowBox[{
    FractionBox["1", "6"], " ", 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"x", "-", "\[Pi]"}], ")"}], "3"]}], "+", 

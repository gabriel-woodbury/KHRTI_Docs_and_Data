Cell[BoxData[
 RowBox[{
  RowBox[{"Options", "[", "RootSearch", "]"}], "//", "TableForm"}]], "Input",E\
xpressionUUID->"46c84414-5a36-204b-af2a-31d104de854a"],

Cell[BoxData[
 InterpretationBox[GridBox[{
    {
     RowBox[{"InitialSamples", "\[Rule]", "300"}]},
    {
     RowBox[{"RootTest", "\[RuleDelayed]", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"Abs", "[", "#2", "]"}], "\[LessEqual]", 
         RowBox[{
          SuperscriptBox["10", "5"], " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Abs", "[", "#1", "]"}], "+", "$MinMachineNumber"}], 
           ")"}], " ", "$MachineEpsilon"}]}], "&"}], ")"}]}]},
    {
     RowBox[{"MaxBrentSteps", "\[Rule]", "150"}]},
    {
     RowBox[{"MaxSecantSteps", "\[Rule]", "150"}]},
    {
     RowBox[{"PrecisionGoal", "\[RuleDelayed]", "$MachinePrecision"}]},
    {
     RowBox[{"InitialPrecision", "\[RuleDelayed]", "$MachinePrecision"}]}
   },
   GridBoxAlignment->{"Columns" -> {{Left}}, "Rows" -> {{Baseline}}},
   GridBoxSpacings->{"Columns" -> {
       Offset[0.27999999999999997`], {
        Offset[2.0999999999999996`]}, 
       Offset[0.27999999999999997`]}, "Rows" -> {
       Offset[0.2], {
        Offset[0.4]}, 
       Offset[0.2]}}],
  TableForm[{
   InitialSamples -> 300, 
    RootTest :> (Abs[#2] <= 
     10^5 (Abs[#] + $MinMachineNumber) $MachineEpsilon& ), MaxBrentSteps -> 
    150, MaxSecantSteps -> 150, PrecisionGoal :> $MachinePrecision, 
    InitialPrecision :> $MachinePrecision}]]], "Output",ExpressionUUID->\
"1e74da86-fb6d-f042-b8e8-5f57ad0b0041"]
}, Open  ]],


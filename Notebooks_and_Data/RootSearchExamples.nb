 RowBox[{
  RowBox[{"ClearAll", "[", "f6", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"f6", "[", "x_", "]"}], "/;", 
    RowBox[{"x", "<", "1.4"}]}], ":=", " ", 
   SqrtBox[
    RowBox[{
     RowBox[{"Abs", "[", "x", "]"}], "-", "1.2"}]]}], " ", 
  ";"}], "\[IndentingNewLine]", 
